if form then form.Destroy() form = nil end
DP1 = getScreenDPI() / 96
form = createForm()
form.height = 640* DP1
form.width = 473 * DP1
form.left = 400 * DP1
form.top = 109 * DP1
form.PopupMode = 0

form.caption = 'üî±üî•SoL .Gaming.MODE MENU HACKüî•üî±'
form.Position = "poDesktopCenter"
form.ShowInTaskBar = "stAlways"
form.BorderStyle="bsNone"
form.ParentDoubleBuffered = true
form.ParentBiDiMode = true
---form.setLayeredAttributes(0x000100, 255, LWA_COLORKEY | LWA_ALPHA )
---form.Color=0x000100
form.Visible=false
local ftbl = {}
----------------------- ftbl.titlepnl -----
ftbl.titlepnl=createPanel(form)
ftbl.titlepnl.AutoSize=false
ftbl.titlepnl.height=50*DP1
ftbl.titlepnl.width=470*DP1 ftbl.titlepnl.left=0*DP1 ftbl.titlepnl.top=0*DP1
