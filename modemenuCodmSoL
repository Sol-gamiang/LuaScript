DP1 = getScreenDPI() / 96
form = createForm()
form.height = 630 * DP1
form.width = 475 * DP1
form.left = 400 * DP1
form.top = 109 * DP1
form.PopupMode = 0
form.caption = 'üî•SoL .Gaming.CODM MODE MENU HACKüî•'
form.Position = "poDesktopCenter"
form.ShowInTaskBar = "stAlways"
--form.BorderStyle = "bsNone"

local ftbl = {}
ftbl.backPnl1 = createPanel(form)
ftbl.backPnl1.AutoSize = false
ftbl.backPnl1.height = 630 * DP1
ftbl.backPnl1.width = 475 * DP1
ftbl.backPnl1.left = 0 * DP1
