----------------------------------------------------------------------------------------------
function file_exists(file_path)
    local file = io.open(file_path, "r")
    if file then        file:close()        return true
    else        return false
    end
end
function file_readable(file_path)
    local file = io.open(file_path, "r")
    if file then        file:close()        return true
    else        return false
    end
end
sleep(500)
local function deleteFile(file_path)
    local success, err_msg = os.remove(file_path)

    if success then
        showMessage("üí†‚öúÔ∏èüí†‚öúÔ∏èüí†‚öúÔ∏è   AnTi       BANNED    ACTIVE   üí†‚öúÔ∏èüí†‚öúÔ∏è‚öúÔ∏èüí†")
    else
        showMessage("NO ACCESS. PLEASE FIRST RUN USER.EXE")
    end
end
local temp_path = os.getenv("TEMP") if temp_path then
    local file_path = temp_path .. "\\WindowsReport data.txt"
    if not file_exists(file_path) then
        showMessage("SORY .NO ACCESS.")
        os.exit()
    elseif not file_readable(file_path) then
        showMessage("                  üí†SORY.PLEASE INSERT PASWORD .NO ACCESSüí† ")
        os.exit()
    end
end
local function deleteFile(file_path)
    local success, err_msg = os.remove(file_path)
    if success then
        showMessage("üí†‚öúÔ∏èüí†‚öúÔ∏èüí†‚öúÔ∏è   AnTi       BANNED     ACTIVE   üí†‚öúÔ∏èüí†‚öúÔ∏è‚öúÔ∏èüí†")
       end
end
local temp_path = os.getenv("TEMP")
if temp_path then
    local file_path = temp_path .. "\\WindowsReport data.txt"
    deleteFile(file_path)
end
sleep(2000)
----------------------------------------------------------------
local temp_path = os.getenv("TEMP") if temp_path then
    local file_path = temp_path .. "\\WindowsReport data.txt"
    local file = io.open(file_path, "r")
    if not file then        showMessage("üí†NO ACCESS. PLEASE FIRST RUN ACCESS.exe(Error^55^)üí†")
        os.exit()
    end
    local progay = file:read("*n")    file:close()    if progay then
        showMessage("                       ‚öúÔ∏èüí†‚öúÔ∏èWELLCOME  üî• ÿÆŸàÿ¥ ÿ¢ŸÖÿØ€åÿØ‚öúÔ∏èüí†‚öúÔ∏è")
        openProcess(progay)
    end
end
