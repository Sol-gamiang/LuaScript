local message = "                                   USER ONLINE \n"
local currentTime = os.date("                                                %H:%M:%S")
local currentDate = os.date("                                  %A, %B %d, %Y")


message = message .. "".. currentDate .. "\n"
message = message .. "".. currentTime
showMessage(message)
