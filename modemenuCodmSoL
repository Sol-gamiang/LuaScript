    function RedLineON()
	searchV = '00 00 80 40 00 00 80 3F 00 00 80 3F 00 00 00 00'
	replaceV = '00 00 48 42 00 00 80 3F 00 00 96 43 00 00 00 00'
	AOBRep(searchV,replaceV)
    	showMessage("‚öúÔ∏è‚öúÔ∏è‚öúÔ∏èRed   Line     Activad‚öúÔ∏è‚öúÔ∏è‚öúÔ∏è")
        	end
	function RedLineOFF()
	searchV = '00 00 48 42 00 00 80 3F 00 00 96 43 00 00 00 00'
	replaceV = '00 00 80 40 00 00 80 3F 00 00 80 3F 00 00 00 00'
	AOBRep(searchV,replaceV)
    	showMessage("‚öúÔ∏è‚öúÔ∏è‚öúÔ∏èRed   Line     Disabled‚öúÔ∏è‚öúÔ∏è‚öúÔ∏è")
        	end
